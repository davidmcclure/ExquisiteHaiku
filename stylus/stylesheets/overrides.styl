p
  font-family inherit
  color black

.btn
  font-family "Bowlby One SC"
  border 0px
  border-radius 2px

input[type='text']
input[type='password']
textarea
  display block
  box-shadow none
  border-radius 1px
  font-family inherit
  width 150px

  &:focus
    box-shadow none

.help-inline
  font-size 13px

.form-actions
  background transparent
  border none
  padding 0
